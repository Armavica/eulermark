{
  "time": "/usr/bin/time",
  "valid_extensions": [".c", ".cpp", ".coffee", ".hs", ".py", ".rb", ".rs"],
  "timeout": 60,
  "language": {
    ".c": "C",
    ".cpp": "C++",
    ".coffee": "CoffeeScript",
    ".hs": "haskell",
    ".py": "python",
    ".rb": "ruby",
    ".rs": "rust"
  },
  "compiler": {
    ".c": ["gcc", "-std=c11"],
    ".cpp": ["g++", "-std=c++11"],
    ".coffee": ["coffee", "-c"],
    ".hs": ["ghc"],
    ".py": ["python3.3", "-m"],
    ".rs": ["rustc"]
  },
  "input_needs_extension": [".c", ".cpp", ".coffee", ".hs", ".rs"],
  "interpreter": {
    ".coffee": ["node"],
    ".py": ["python3.3"],
    ".rb": ["ruby"]
  },
  "interpreter_extension": {
    ".coffee": ".js",
    ".py": ".py",
    ".rb": ".rb"
  },
  "byproduct": {
    ".c": [""],
    ".cpp": [""],
    ".coffee": [".js"],
    ".hs": ["", ".hi", ".o"],
    ".py": "__pycache__",
    ".rs": [""]
  },
  "comment": {
    ".c": "//",
    ".cpp": "//",
    ".coffee": "#",
    ".hs": "--",
    ".py": "#",
    ".rb": "#",
    ".rs": "//"
  }
}

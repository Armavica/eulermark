{
  "valid_extensions": [".c", ".cpp", ".hs", ".py", ".rb", ".rs"],
  "timeout": 60,
  "language": {
    ".c": "C",
    ".cpp": "C++",
    ".hs": "haskell",
    ".py": "python",
    ".rb": "ruby",
    ".rs": "rust"
  },
  "compiler": {
    ".c": ["gcc", "-std=c11"],
    ".cpp": ["g++", "-std=c++11"],
    ".hs": ["ghc"],
    ".py": ["python3.3", "-m"],
    ".rs": ["rustc"]
  },
  "input_needs_extension": [".c", ".cpp", ".hs", ".rs"],
  "interpreter": {
    ".py": ["python3.3"],
    ".rb": ["ruby"]
  },
  "output_needs_extension": [ ".py", ".rb" ],
  "byproduct": {
    ".c": [""],
    ".cpp": [""],
    ".hs": ["", ".hi", ".o"],
    ".py": "__pycache__",
    ".rs": [""]
  },
  "comment": {
    ".c": "//",
    ".cpp": "//",
    ".hs": "--",
    ".py": "#",
    ".rb": "#",
    ".rs": "//"
  }
}
